	],
	"contact": {
		"sources": "https://github.com/Yorick-06/DatapackUIs",
		"homepage": "https://github.com/Yorick-06/DatapackUIs",
		"issues": "https://github.com/Yorick-06/DatapackUIs/issues"
	},
